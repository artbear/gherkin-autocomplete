{
	"extends": [
        "tslint:latest",
        "tslint-sonarts"
    ],
	"rules": {
		"no-var-requires": false,
		"no-console": [
			false
		],
		"trailing-comma": [
			false
		],
		"semicolon": true,
        "object-literal-sort-keys": false,
        "forin": false,
        "no-implicit-dependencies": [true, "dev"],
        "deprecation": true
	}
}